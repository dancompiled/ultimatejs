{
  "compilerOptions": {
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noUncheckedIndexedAccess": true,
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    
  },
  "exclude": ["node_modules"],
  "include": ["server"]
}